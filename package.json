{
    "name": "swaggerlint",
    "version": "0.0.7",
    "description": "Swaggerlint  helps you to have a consistent API style by linting your swagger / OpenAPI Scheme.",
    "main": "./dist/index.js",
    "scripts": {
        "build": "rm -rf ./dist && tsc",
        "types": "tsc --noEmit",
        "lint": "eslint . --ext=.js,.ts",
        "dev": "ts-node ./src/bin.ts",
        "test": "jest",
        "preversion": "npm run test && npm run lint && npm run build",
        "postversion": "npm publish && git push --follow-tags"
    },
    "bin": {
        "swaggerlint": "./dist/bin.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/antonk52/swaggerlint.git"
    },
    "keywords": [
        "swagger",
        "openapi"
    ],
    "author": "antonk52",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/antonk52/swaggerlint/issues"
    },
    "devDependencies": {
        "@types/jest": "^24.9.1",
        "@types/js-yaml": "^3.12.4",
        "@types/lodash.get": "^4.4.6",
        "@types/lodash.merge": "^4.6.6",
        "@types/mime-db": "^1.43.0",
        "@types/minimist": "^1.2.0",
        "@types/node": "^12.12.47",
        "@types/node-fetch": "^2.5.7",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^24.9.0",
        "lodash.merge": "^4.6.2",
        "prettier": "^1.19.1",
        "ts-jest": "^24.3.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "case": "^1.6.2",
        "cosmiconfig": "^6.0.0",
        "js-yaml": "^3.13.1",
        "kleur": "^3.0.3",
        "lodash.get": "^4.4.2",
        "mime-db": "^1.43.0",
        "minimist": "^1.2.0",
        "node-fetch": "^2.6.0"
    }
}
